{
    "Name": "SERVICE_NAME",
    "TaskTemplate": {
        "ContainerSpec": {
            "Image": "mysql:VERSION",
            "Hostname": "HOSTNAME",
            "Env": [
                "MYSQL_ROOT_PASSWORD=DB_PASS",
                "MYSQL_DATABASE=DB_NAME"
            ],
            "DNSConfig": {
                "Nameservers": [
                    "8.8.8.8"
                ]
            }
        },
        "Resources": {
            "Limits": {
                "NanoCPUs": NANO_CPU,
                "MemoryBytes": MEMORY_BYTE
            }
        },
        "RestartPolicy": {
            "Condition": "on-failure",
            "Delay": 10000000000,
            "MaxAttempts": 10
        }
    },
    "Mode": {
        "Replicated": {
            "Replicas": 1
        }
    },
    "UpdateConfig": {
        "Parallelism": 1,
        "Delay": 1000000000,
        "FailureAction": "rollback",
        "Monitor": 15000000000,
        "MaxFailureRatio": 0.15
    },
    "RollbackConfig": {
        "Parallelism": 1,
        "Delay": 1000000000,
        "FailureAction": "rollback",
        "Monitor": 15000000000,
        "MaxFailureRatio": 0.15
    },
    "EndpointSpec": {
        "Ports": [
            {
                "Protocol": "tcp",
                "PublishedPort": PUBLISHED_PORT,
                "TargetPort": 3306
            }
        ]
    }
}