{
  "Name": "phpmyadmin-temp",
  "TaskTemplate": {
    "ContainerSpec": {
      "Image": "phpmyadmin/phpmyadmin:4.9",
			"Env": [
				"PMA_ARBITRARY=1",
				"PMA_HOST=192.168.2.29",
				"PMA_PORT=33344",
				"HIDE_PHP_VERSION=true",
				"UPLOAD_LIMIT=2048kb"
			],
			"DNSConfig": {
				"Nameservers": [
					"8.8.8.8"
				]
			}
		},
		"Resources": {
			"Limits": {
		  	"NanoCPUs": 2000000000,
				"MemoryBytes": 1073741824
			}
		},
		"RestartPolicy": {
			"Condition": "on-failure",
        "Delay": 10000000000,
        "MaxAttempts": 10
      }
    },
    "Mode": {
      "Replicated": {
        "Replicas": 2
      }
    },
    "UpdateConfig": {
      "Parallelism": 1,
      "Delay": 1000000000,
      "FailureAction": "rollback",
      "Monitor": 15000000000,
      "MaxFailureRatio": 0.15
    },
    "RollbackConfig": {
      "Parallelism": 1,
      "Delay": 1000000000,
      "FailureAction": "rollback",
      "Monitor": 15000000000,
      "MaxFailureRatio": 0.15
    },
    "EndpointSpec": {
      "Ports": [
        {
          "Protocol": "tcp",
          "PublishedPort": 9000,
          "TargetPort": 80
        }
      ]
    }
}
