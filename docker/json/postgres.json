{
  "Name": "ps1",
  "TaskTemplate": {
    "ContainerSpec": {
      "Image": "postgres:12.4",
			"Env": [
				"POSTGRES_PASSWORD=123456789"
			],
			"DNSConfig": {
				"Nameservers": [
					"8.8.8.8"
				]
			}
		},
		"Resources": {
			"Limits": {
		  	"NanoCPUs": 2000000000,
				"MemoryBytes": 1073741824
			}
		},
		"Placement": {
			"Constraints": ["node.hostname==debian"]
		},
		"RestartPolicy": {
			"Condition": "on-failure",
        "Delay": 10000000000,
        "MaxAttempts": 10
      }
    },
    "Mode": {
      "Replicated": {
        "Replicas": 2
      }
    },
    "UpdateConfig": {
      "Parallelism": 1,
      "Delay": 1000000000,
      "FailureAction": "rollback",
      "Monitor": 15000000000,
      "MaxFailureRatio": 0.15
    },
    "RollbackConfig": {
      "Parallelism": 1,
      "Delay": 1000000000,
      "FailureAction": "rollback",
      "Monitor": 15000000000,
      "MaxFailureRatio": 0.15
    },
    "EndpointSpec": {
      "Ports": [
        {
          "Protocol": "tcp",
          "PublishedPort": 33333,
          "TargetPort": 5432
        }
      ]
    }
}
